<Window x:Class="MegaProduction.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="16*"/>
            <RowDefinition Height="16*"/>
        </Grid.RowDefinitions>


        <Button Grid.Row="0" Padding="5" x:Name="btn_client" Click="Client_Click">Clients</Button>
        <Button Grid.Row="1" Padding="5" x:Name="btn_offre" Click="Offre_Click">Offres</Button>
        <Button Grid.Row="2" Padding="5" x:Name="btn_pack" Click="Pack_Click">Packs</Button>
        <Button Grid.Row="3" Padding="5" x:Name="btn_partenaire" Click="Partenaire_Click">Partenaires</Button>
        <Button Grid.Row="4" Padding="5" x:Name="btn_domaine_metier" Click="Domaine_Metier_Click">Domaines/Métiers</Button>
        <Button Grid.Row="5" Padding="5" x:Name="btn_type_contrat" Click="Type_Contrat_Click">Type Contrat</Button>
    </Grid>
</Window>
